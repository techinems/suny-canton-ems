
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 String             @id @default(cuid())
  email              String             @unique
  firstName          String?            @map("first_name")
  lastName           String?            @map("last_name")
  preferredName      String?            @map("preferred_name")
  shirtSize          ShirtSize?         @map("shirt_size")
  dob                DateTime?          @db.Date
  cantonEmail        String?            @map("canton_email")
  position           Position           @default(MEMBER)
  major              String?
  membershipStanding MembershipStanding @default(GOOD) @map("membership_standing")
  cantonCardId       String?            @map("canton_card_id")
  gpa                Float?
  phoneNumber        String?            @map("phone_number")
  medicalLevel       MedicalLevel?      @map("medical_level")
  housingType        HousingType?       @map("housing_type")
  building           String?
  roomNumber         Int?               @map("room_number")
  homeAddress        String?            @map("home_address")
  localAddress       String?            @map("local_address")
  createdAt          DateTime           @default(now()) @map("created_at")
  emailVerified      Boolean            @default(false) @map("email_verified")
  image              String?
  name               String?
  updatedAt          DateTime           @updatedAt @map("updated_at")
  accounts           Account[]
  callsAsCrewChief   CallLog[]          @relation("CrewChiefCalls")
  certifications     Certification[]
  complianceChecks   ComplianceCheck[]
  sessions           Session[]
  callsAsMember      CallLog[]          @relation("CallMembers")

  @@map("users")
}

model Account {
  id                    String    @id @default(cuid())
  userId                String    @map("user_id")
  accountId             String    @map("account_id")
  providerId            String    @map("provider_id")
  accessToken           String?   @map("access_token")
  refreshToken          String?   @map("refresh_token")
  idToken               String?   @map("id_token")
  accessTokenExpiresAt  DateTime? @map("access_token_expires_at")
  refreshTokenExpiresAt DateTime? @map("refresh_token_expires_at")
  scope                 String?
  password              String?
  createdAt             DateTime  @default(now()) @map("created_at")
  updatedAt             DateTime  @updatedAt @map("updated_at")
  user                  User      @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@map("account")
}

model Session {
  id        String   @id @default(cuid())
  userId    String   @map("user_id")
  token     String   @unique
  expiresAt DateTime @map("expires_at")
  ipAddress String?  @map("ip_address")
  userAgent String?  @map("user_agent")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@map("session")
}



model CallLog {
  id                String    @id @default(cuid())
  callDate          DateTime? @map("call_date") @db.Date
  enRouteDate       DateTime? @map("en_route_date") @db.Date
  onSceneDate       DateTime? @map("on_scene_date") @db.Date
  clearDate         DateTime? @map("clear_date") @db.Date
  location          String?
  nature            String?
  transportRequired Boolean   @default(false) @map("transport_required")
  crewChiefId       String?   @map("crew_chief_id")
  memberIds         String[]  @map("member_ids")
  notes             String?
  created           DateTime  @default(now())
  updated           DateTime  @updatedAt
  crewChief         User?     @relation("CrewChiefCalls", fields: [crewChiefId], references: [id])
  members           User[]    @relation("CallMembers")

  @@map("call_log")
}

model Certification {
  id             String    @id @default(cuid())
  name           String
  userId         String    @map("user_id")
  document       String?
  issueDate      DateTime? @map("issue_date") @db.Date
  expirationDate DateTime? @map("expiration_date") @db.Date
  issuingAgency  String?   @map("issuing_agency")
  certNumber     String?   @map("cert_number")
  created        DateTime  @default(now())
  updated        DateTime  @updatedAt
  user           User      @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@map("certifications")
}

model ComplianceCheck {
  id        String   @id @default(cuid())
  userId    String   @map("user_id")
  checkDate DateTime @map("check_date") @db.Date
  created   DateTime @default(now())
  updated   DateTime @updatedAt
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@map("compliance_checks")
}

model Inventory {
  id             String    @id @default(cuid())
  name           String
  description    String?
  category       String?
  quantity       Int
  location       String?
  purchaseDate   DateTime? @map("purchase_date") @db.Date
  expirationDate DateTime? @map("expiration_date") @db.Date
  cost           Float?
  supplier       String?
  critical       Boolean   @default(false)
  lastChecked    DateTime? @map("last_checked") @db.Date
  created        DateTime  @default(now())
  updated        DateTime  @updatedAt

  @@map("inventory")
}

enum ShirtSize {
  XS
  S
  M
  L
  XL
  XXL
  XXXL
}

enum Position {
  PRESIDENT
  VICE_PRESIDENT
  SECRETARY
  TREASURER
  CAPTAIN
  LIEUTENANT
  MEMBER
  PROBATIONARY_MEMBER
  ALUMNI
}

enum MembershipStanding {
  GOOD
  PROBATION
  INACTIVE
}

enum MedicalLevel {
  EMR
  EMT
  AEMT
}

enum HousingType {
  ON_CAMPUS
  OFF_CAMPUS
  COMMUTER
}

model Verification {
  id         String    @id @default(cuid())
  identifier String
  value      String
  expiresAt  DateTime  @map("expires_at")
  createdAt  DateTime? @default(now()) @map("created_at")
  updatedAt  DateTime? @updatedAt @map("updated_at")

  @@map("verification")
}
